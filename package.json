{
  "name": "netinfo",
  "version": "0.1.0",
  "main": "build",
  "description": "Get network connection details of device in browser",
  "repository": {
    "type": "git",
    "url": "https://github.com/ganapativs/netinfo.git"
  },
  "author": "Ganapati V S",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ganapativs/netinfo/issues"
  },
  "homepage": "https://github.com/ganapativs/netinfo",
  "keywords": [
    "netinfo",
    "browser",
    "internet-info",
    "connection-speed",
    "connection-type"
  ],
  "scripts": {
    "clean": "rm -rf build && rm -rf minified",
    "prebuild": "yarn run clean",
    "build": "babel --plugins 'transform-es2015-modules-umd' src --out-dir ./build",
    "postbuild": "minify build -d minified",
    "prepublish": "yarn run clean && yarn run build",
    "lint": "eslint ./src",
    "lintfix": "eslint ./src --fix"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.1",
    "babel-minify": "^0.2.0",
    "babel-plugin-transform-es2015-modules-umd": "^6.24.1",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "eslint": "^4.9.0",
    "eslint-plugin-babel": "^4.1.2"
  }
}
